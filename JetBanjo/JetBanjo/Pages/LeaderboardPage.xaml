<?xml version="1.0" encoding="utf-8" ?>
<c:CContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:JetBanjo;assembly=JetBanjo"
             xmlns:i18n="clr-namespace:JetBanjo;assembly=JetBanjo"
             xmlns:c="clr-namespace:JetBanjo.Pages;assembly=JetBanjo"
             x:Class="JetBanjo.Pages.LeaderboardPage"
             Title="{i18n:Translate leaderboard}">
    
    <ContentPage.Content>
        <StackLayout>
            <ListView x:Name="leaderboardListView">
                <ListView.ItemTemplate>
                    <DataTemplate>
                        <TextCell Text="{Binding LeaderboardString}" />
                    </DataTemplate>
                </ListView.ItemTemplate>
            </ListView>
        </StackLayout>
    </ContentPage.Content>

</c:CContentPage>